{
  "name": "web-automation-system",
  "version": "2.0.0-dev",
  "description": "Automated web interaction system with realtime data processing",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon src/main.js",
    "dev:debug": "node --inspect src/main.js",
    "test": "node src/test.js",
    "clean": "rimraf logs/*.log results/*.json data/grouped-*.json",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ERO-Automation",
    "pm2:restart": "pm2 restart ERO-Automation",
    "pm2:delete": "pm2 delete ERO-Automation",
    "pm2:logs": "pm2 logs ERO-Automation",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "setup": "npm install && npx playwright install chromium"
  },
  "dependencies": {
    "playwright": "^1.40.0",
    "chokidar": "^3.5.3",
    "winston": "^3.11.0",
    "crypto": "^1.0.1",
    "fs-extra": "^11.1.1",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
