{
  "sites": [
    {
      "name": "my-site",
      "url": "https://ero-partner.asia.cma-cgm.com/login",
      "credentials": {
        "username": "data.account.username",
        "password": "data.account.password"
      },
      "selectors": {
        "confirm_button": ".el-button.el-button--danger",
        "login_button": ".btn.btn-login.forgotten.cma-external",
        "username_field": "#username",
        "password_field": "#password"
      },
      "actions": [
        {
          "type": "click",
          "selector": ".el-button.el-button--danger",
          "print_url": true,
          "delay": 1000
        },
        {
          "type": "click",
          "selector": ".btn.btn-login.forgotten.cma-external",
          "print_url": true,
          "delay": 1000
        },
        {
          "type": "input",
          "selector": "#username",
          "value": "data.account.username",
          "delay": 500
        },
        {
          "type": "input",
          "selector": "#password",
          "value": "data.account.password",
          "delay": 500
        },
        {
          "type": "click",
          "selector": ".btn.btn-login.cma-external",
          "print_url": true,
          "delay": 1000
        },
        {
          "type": "input",
          "selector": "#releaseOrderNumber",
          "value": "data.releaseOrderNumber",
          "delay": 500
        },
        {
          "type": "click",
          "selector": "#btn_ValidateRO",
          "delay": 1000
        }
      ]
    }
  ]
}
